/**index.wxss**/
.header {
  width: 100%;
  height: 53rpx;
  text-align: center;
  background: #fff;
  font-size: 33rpx;
  color: #00156f;
}

.list-container {
  background: #fff;
  width: 710rpx;
  display: flex;
  flex-direction: column;
  margin-top: 12rpx;
}

.list-header {
  display: flex;
  flex-direction: row;
  justify-content: space-between;
  font-size: 21rpx;
  color: #727272;
  margin-top: 34rpx;
}

.order-text {
  margin-left: 26rpx;
}

.order-status {
  margin-right: 26rpx;
}

.line {
  width: 660rpx;
  background-color: #bbbbbb;
  height: 1rpx;
  margin: 0 auto;
  margin-top: 16rpx;
  margin-bottom: 16rpx;
}

.list-middle {
  display: flex;
  flex-direction: column;
  font-size: 25rpx;
  margin-left: 26rpx;
}

.list-card {
  display: inline-block;
  margin-bottom: 29rpx;
}

.list-middle-row {
  display: flex;
  flex-direction: row;
  justify-content: space-between;
}

.goods {
  margin-right: 26rpx;
}

.created-time {
  color: #727272;
  margin-top: 16rpx;
}

button {
  display: block;
  color: #fff;
  width: 100rpx;
  height: 44rpx;
  font-size: 25rpx;
  text-align: center;
  padding: 0;
  line-height: 44rpx;
}

.button-container {
  display: flex;
  flex-direction: row;
  justify-content: flex-end;
  margin-bottom: 34rpx;
}

.edit {
  background-color: #00156f;
}

.see {
  margin-left: 49rpx;
}

.delete {
  background-color: #a60101;
  margin-left: 49rpx;
  margin-right: 26rpx;
} 

.plus-icon {
  width: 80rpx;
  height: 80rpx;
  text-align: center;
  position: fixed;
  top: 920rpx;
  right: 26rpx;
  z-index: 1000;
}